{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables default capabilities",
  "windows": ["launcher", "splash-screen"],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-minimize",
    "core:window:allow-close",
    "core:window:allow-start-dragging"
  ]
}
